{
    "name": "geekshubs/rabbitmq",
    "description": "Package from RabbitMQ adapter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Geekshubs",
            "email": "desarrollo@geekshubs.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "php-amqplib/php-amqplib": "^2.12"
    },
    "require-dev": {
        "laravel/framework": "^6|^7|^8",
        "phpunit/phpunit": "^6|^7|^8",
        "illuminate/support": "^6|^7|^8|^9",
        "illuminate/contracts": "^6|^7|^8|^9",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench-dusk": "^4|^5|^6"
    },
    "autoload": {
        "psr-4": {
            "Geekshubs\\RabbitMQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekshubs\\RabbitMQ\\Tests\\": "tests/"
        }
    },
    "extra": {
      "laravel": {
            "providers": [
                "Geekshubs\\RabbitMQ\\RabbitMQServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
